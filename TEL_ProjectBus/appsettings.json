{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:5000"
      }
    }
  },
  "UseInMemoryTransport": true,
  "RabbitMq": {
    "Host": "rabbitmq://user:pass@localhost:5672",
    "User": "user",
    "Password": "pass"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://app.company.com",
      "https://admin.company.com"
    ]
  },
  "UseDbSeed": false
}
